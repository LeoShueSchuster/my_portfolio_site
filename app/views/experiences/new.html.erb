<%= link_to("Go Back to Home", root_path) %>
<h5><br>Create a new Experience</h5>

<%= form_for(@experience) do |f| %>
  <p>
    <%= f.label :title %><br/>
    <%= f.text_field :title, size: 40 %>
  </p>
    <p>
    <%= f.label :date %><br/>
    <%= f.text_field :date, size: 40 %>
  </p>
  <p>
    <%= f.label :location %><br/>
    <%= f.text_field :location, size: 40 %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>